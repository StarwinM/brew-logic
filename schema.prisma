datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Drink {
  id          Int    @id @default(autoincrement())
  name        String @unique
  amount      Int
  description String
}

model Type {
  id   Int    @id @default(autoincrement())
  name String @unique
}
